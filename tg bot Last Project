iimport telebot
import datetime
import time
import threading
from telebot import types
from random import choice

temporary_event_names = dict()
completed_constant_event_names = list()
incomplete_constant_event_names = list()
bot = telebot.TeleBot('Token')
the_most_frequent_temporary_event = ''
count_the_most_frequent_temporary_event = 0
the_most_frequent_constant_event = ''
count_the_most_frequent_constant_event = 0


def ya_zabil(message, event_time, event_name):
    now = datetime.datetime.now()
    delta = event_time - now
    for i in range(int(delta.total_seconds() // 3600)):
        event_timer = threading.Timer((delta.total_seconds() - i * 3600), send_temporary_event,
                                      [message.chat.id, f'  —É –≤–∞—Å –Ω–∞–∑–Ω–∞—á–µ–Ω–æ —Å–æ–±—ã—Ç–∏–µ {event_name}'])
        event_timer.start()


@bot.message_handler(commands=['dice'])
def dice(message):
    send_kubik(message.chat.id)


def send_kubik(chat_id):
    value = bot.send_dice(chat_id, emoji='üé≤').dice.value
    time.sleep(.5)
    bot.send_message(chat_id, '–í–∞–º –≤—ã–ø–∞–ª–æ...')
    time.sleep(5)
    bot.send_message(chat_id, f'–ß–∏—Å–ª–æ {value}!')


def most_frequent_temporary_event(message):
    global the_most_frequent_temporary_event
    global count_the_most_frequent_temporary_event
    bot.send_message(message.chat.id, f'–°–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ {the_most_frequent_temporary_event}')
    bot.send_message(message.chat.id, f'–û–Ω–æ –ø–æ–≤—Ç–æ—Ä—è–ª–æ—Å—å {count_the_most_frequent_temporary_event} —Ä–∞–∑')


def most_frequent_constant_event(message):
    global the_most_frequent_constant_event
    global count_the_most_frequent_constant_event
    bot.send_message(message.chat.id, f'–°–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ {the_most_frequent_constant_event}')
    bot.send_message(message.chat.id, f'–û–Ω–æ –ø–æ–≤—Ç–æ—Ä—è–ª–æ—Å—å {count_the_most_frequent_constant_event} —Ä–∞–∑')


@bot.message_handler(commands=['temporary_events_status'])
def temporary_events_status(message):
    global count_the_most_frequent_temporary_event
    global the_most_frequent_temporary_event
    try:
        value_list = list(temporary_event_names.values())
        key_list = list(temporary_event_names.keys())
        if len(key_list) > 0:
            for i in range(len(key_list)):
                bot.send_message(message.chat.id, f'–°–æ–±—ã—Ç–∏–µ {key_list[i]} –¥–ª–∏–ª–æ—Å—å - {value_list[i]}')
            for j in range(len(key_list)):
                if key_list.count(key_list[j]) >= count_the_most_frequent_temporary_event:
                    the_most_frequent_temporary_event = key_list[j]
                    count_the_most_frequent_temporary_event = key_list.count(key_list[j])
            markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
            btn1 = types.KeyboardButton("–°–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
            btn2 = types.KeyboardButton("–í–µ—Ä–Ω—É—Ç—å—Å—è –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é")
            markup.add(btn1, btn2)
            bot.send_message(message.chat.id, "–¢–µ–ø–µ—Ä—å –≤—ã –º–æ–∂–µ—Ç–µ —É–≤–∏–¥–µ—Ç—å —Å–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ",
                             reply_markup=markup)
        else:
            bot.send_message(message.chat.id, '–û—à–∏–±–∫–∞, –£ –≤–∞—Å –Ω–µ—Ç –≤—Ä–µ–º–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π')
    except ValueError:
        bot.send_message(message.chat.id, '–û—à–∏–±–∫–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞')


@bot.message_handler(commands=['constant_events_status'])
def constant_events_status(message):
    global count_the_most_frequent_constant_event
    global the_most_frequent_constant_event
    try:
        comp = completed_constant_event_names[:]
        incomp = incomplete_constant_event_names[:]
        if len(comp) > 0:
            bot.send_message(message.chat.id, '–ó–∞–≤–µ—Ä—à—ë–Ω–Ω—ã–µ —Å–æ–±—ã—Ç–∏—è:')
            for i in range(len(comp)):
                bot.send_message(message.chat.id, f'{i + 1}) {comp[i]}')
            for j in range(len(comp)):
                if comp.count(comp[j]) >= count_the_most_frequent_constant_event:
                    the_most_frequent_constant_event = comp[j]
                    count_the_most_frequent_constant_event = comp.count(comp[j])
            markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
            btn1 = types.KeyboardButton("–°–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
            btn2 = types.KeyboardButton("–í–µ—Ä–Ω—É—Ç—å—Å—è –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é")
            markup.add(btn1, btn2)
            bot.send_message(message.chat.id, "–¢–µ–ø–µ—Ä—å –≤—ã –º–æ–∂–µ—Ç–µ —É–≤–∏–¥–µ—Ç—å —Å–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ",
                             reply_markup=markup)
        else:
            bot.send_message(message.chat.id, '–û—à–∏–±–∫–∞, –£ –≤–∞—Å –Ω–µ—Ç –∑–∞–≤–µ—Ä—à—ë–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π')
        if len(incomp) > 0:
            bot.send_message(message.chat.id, '–ù–µ–∑–∞–≤–µ—Ä—à—ë–Ω–Ω—ã–µ —Å–æ–±—ã—Ç–∏—è:')
            for i in range(len(incomp)):
                bot.send_message(message.chat.id, f'{i + 1}) {incomp[i]}')
        else:
            bot.send_message(message.chat.id, '–û—à–∏–±–∫–∞, –í—Å–µ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏–µ —Å–æ–±—ã—Ç–∏—è –±—ã–ª–∏ –∑–∞–≤–µ—Ä—à–µ–Ω—ã –ª–∏–±–æ –æ–Ω–∏ –æ—Ç—Å—É—Ç—Å–≤—É—é—Ç')
    except ValueError:
        bot.send_message(message.chat.id, '–û—à–∏–±–∫–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞')


@bot.message_handler(commands=['start'])
def start_message(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    btn1 = types.KeyboardButton("–ü—Ä–∏–≤–µ—Ç")
    btn2 = types.KeyboardButton("–ó–∞–¥–∞—Ç—å –≤–æ–ø—Ä–æ—Å")
    btn3 = types.KeyboardButton("–°–æ–∑–¥–∞—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
    btn6 = types.KeyboardButton("–°–æ–∑–¥–∞—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")

    markup.add(btn1, btn2, btn3, btn6)
    bot.send_message(message.chat.id,
                     "–ü—Ä–∏–≤–µ—Ç, {0.first_name}! –≠—Ç–æ –±–æ—Ç –¢—Ä—ç–∫–µ—Ä –∑–∞–¥–∞—á, –æ–Ω –æ—Ç—Å–ª–µ–∂–∏–≤–∞–µ—Ç –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å —Ç–µ—Ö –∏–ª–∏ "
                     "–∏–Ω—ã—Ö –¥–µ–π—Å—Ç–≤–∏–π)".format(message.from_user),
                     reply_markup=markup)


@bot.message_handler(commands=['temporary_event'])
def temporary_event_message(message):
    # –ó–∞–ø—Ä–∞—à–∏–≤–∞–µ–º —É –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è –∏ –¥–∞—Ç—É –∏ –≤—Ä–µ–º—è —Å–æ–±—ã—Ç–∏—è
    bot.send_message(message.chat.id, '–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è:')
    bot.register_next_step_handler(message, set_temporary_event_name)


def set_temporary_event_name(message):
    user_data = {}
    user_data[message.chat.id] = {'temporary_event_name': message.text}
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    btn3 = types.KeyboardButton("–°–æ–∑–¥–∞—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
    back = types.KeyboardButton("–í–µ—Ä–Ω—É—Ç—å—Å—è –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é")
    markup.add(back, btn3)
    bot.send_message(message.chat.id,
                     "–í–≤–µ–¥–∏—Ç–µ –¥–∞—Ç—É –∏ –≤—Ä–µ–º—è, –∫–æ–≥–¥–∞ –≤—ã —Ö–æ—Ç–∏—Ç–µ –ø–æ–ª—É—á–∏—Ç—å —Å–æ–±—ã—Ç–∏–µ –≤ —Ñ–æ—Ä–º–∞—Ç–µ –ì–ì–ì–ì-–ú–ú-–î–î —á—á:–º–º:—Å—Å.".format(
                         message.from_user),
                     reply_markup=markup)
    curr_time = time.strftime('%Y-%m-%d %H:%M:%S', time.localtime())
    bot.send_message(message.chat.id, f'–ù–∞–ø—Ä–∏–º–µ—Ä {curr_time}')
    bot.register_next_step_handler(message, temporary_event_set, user_data)


def temporary_event_set(message, user_data):
    try:
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        btn1 = types.KeyboardButton("–Ø –∑–∞–±—ã–≤—á–∏–≤—ã–π ‚îê('ÔΩû`Ôºõ)‚îå")
        markup.add(btn1)
        event_time = datetime.datetime.strptime(message.text, '%Y-%m-%d %H:%M:%S')
        now = datetime.datetime.now()
        delta = event_time - now
        if delta.total_seconds() <= 0:
            bot.send_message(message.chat.id, '–í—ã –≤–≤–µ–ª–∏ –ø—Ä–æ—à–µ–¥—à—É—é –¥–∞—Ç—É, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–æ–∑–¥–∞—Ç—å —Å–æ–±—ã—Ç–∏–µ –µ—â–µ —Ä–∞–∑.')
        else:
            event_name = user_data[message.chat.id]['temporary_event_name']
            bot.send_message(message.chat.id,
                             '–°–æ–±—ã—Ç–∏–µ "{}" —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ –Ω–∞ {}.'.format(event_name, event_time))
            event_timer = threading.Timer(delta.total_seconds(), send_temporary_event, [message.chat.id, event_name])
            event_timer.start()
            temporary_event_names[event_name] = delta
            if (message.text == "–Ø –∑–∞–±—ã–≤—á–∏–≤—ã–π ‚îê('ÔΩû`Ôºõ)‚îå"):
                ya_zabil(message, event_time, event_name)
    except ValueError:
        bot.send_message(message.chat.id,
                         '–í—ã –≤–≤–µ–ª–∏ –Ω–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç –¥–∞—Ç—ã –∏ –≤—Ä–µ–º–µ–Ω–∏, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–æ–∑–¥–∞—Ç—å —Å–æ–±—ã—Ç–∏–µ –µ—â–µ —Ä–∞–∑.')


@bot.message_handler(commands=['del_event'])
def event_del(message):
    bot.send_message(message.chat.id, '–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è –∫–æ—Ç–æ—Ä–æ–µ –Ω—É–∂–Ω–æ —É–¥–∞–ª–∏—Ç—å:')
    bot.register_next_step_handler(message, del_temporary_events)


def del_temporary_events(message):
    try:
        user_data = {}
        user_data[message.chat.id] = {'temporary_event_for_del': message.text}
        if message.text not in temporary_event_names:
            bot.send_message(message.chat.id, '–¢–∞–∫–æ–≥–æ —Å–æ–±—ã—Ç–∏—è –Ω–µ—Ç')
        else:
            bot.send_message(message.chat.id, f'–°–æ–±—ã—Ç–∏–µ {message.text} —É–¥–∞–ª–µ–Ω–æ')
            del temporary_event_names[message.text]

    except ValueError:
        bot.send_message(message.chat.id, '–í–æ–∑–º–æ–∂–Ω–æ –≤—ã –≤–≤–µ–ª–∏ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è')


def send_temporary_event(chat_id, event_name):
    bot.send_message(chat_id, '–í–Ω–∏–º–∞–Ω–∏–µ! –ù–∞—Å—Ç—É–ø–∏–ª–æ —Å–æ–±—ã—Ç–∏–µ "{}"!'.format(event_name))


@bot.message_handler(commands=['constant_event'])
def constant_event_message(message):
    markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
    btn1 = types.KeyboardButton("–°–æ–∑–¥–∞—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
    back = types.KeyboardButton("–í–µ—Ä–Ω—É—Ç—å—Å—è –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é")
    markup.add(back, btn1)
    bot.send_message(message.chat.id, '–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è:', reply_markup=markup)
    bot.register_next_step_handler(message, constant_event_set)


def constant_event_set(message):
    bot.send_message(message.chat.id,
                     '–°–æ–±—ã—Ç–∏–µ "{}" —Å–æ–∑–¥–∞–Ω–æ'.format(message.text))
    incomplete_constant_event_names.append(message.text)


@bot.message_handler(commands=['del_constant_event'])
def constant_event_del(message):
    bot.send_message(message.chat.id, '–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è –∫–æ—Ç–æ—Ä–æ–µ –Ω—É–∂–Ω–æ —É–¥–∞–ª–∏—Ç—å:')
    bot.register_next_step_handler(message, del_constant_events)


def del_constant_events(message):
    try:
        user_data = {}
        user_data[message.chat.id] = {'constant_event_for_del': message.text}
        constant_event_for_del = user_data[message.chat.id]['constant_event_for_del']
        if constant_event_for_del not in incomplete_constant_event_names:
            if constant_event_for_del not in completed_constant_event_names:
                bot.send_message(message.chat.id, '–¢–∞–∫–æ–≥–æ —Å–æ–±—ã—Ç–∏—è –Ω–µ—Ç')
        else:
            i = incomplete_constant_event_names.index(constant_event_for_del)
            bot.send_message(message.chat.id, f'–°–æ–±—ã—Ç–∏–µ {constant_event_for_del} —É–¥–∞–ª–µ–Ω–æ')
            if constant_event_for_del in incomplete_constant_event_names:
                print(incomplete_constant_event_names[i])
                del incomplete_constant_event_names[i]
            if constant_event_for_del in completed_constant_event_names:
                print(completed_constant_event_names[i])
                del completed_constant_event_names[i]

    except ValueError:
        bot.send_message(message.chat.id, '–í–æ–∑–º–æ–∂–Ω–æ –≤—ã –≤–≤–µ–ª–∏ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è')


@bot.message_handler(commands=['complete_event'])
def complete_event(message):
    bot.send_message(message.chat.id, '–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è –∫–æ—Ç–æ—Ä–æ–µ –Ω—É–∂–Ω–æ –≤—ã–ø–æ–ª–Ω–∏—Ç—å:')
    bot.register_next_step_handler(message, complete_any_event)


def complete_any_event(message):
    try:
        if message.text not in incomplete_constant_event_names:
            bot.send_message(message.chat.id, '–¢–∞–∫–æ–≥–æ —Å–æ–±—ã—Ç–∏—è –Ω–µ—Ç')
        else:
            bot.send_message(message.chat.id, f'–°–æ–±—ã—Ç–∏–µ {message.text} –≤—ã–ø–æ–ª–Ω–µ–Ω–æ')
            i = incomplete_constant_event_names.index(message.text)
            del incomplete_constant_event_names[i]
            completed_constant_event_names.append(message.text)
    except ValueError:
        bot.send_message(message.chat.id, '–í–æ–∑–º–æ–∂–Ω–æ –≤—ã –≤–≤–µ–ª–∏ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å–æ–±—ã—Ç–∏—è')


@bot.message_handler(content_types=['text'])
def func(message):
    if datetime.datetime.now().minute % 5 == 0:
        bot.send_message(message.chat.id, choice(skuka))

    if (message.text == "–ü—Ä–∏–≤–µ—Ç"):
        bot.send_message(message.chat.id, text="–ü—Ä–∏–≤–µ—Ç :)")

    elif (message.text == "–°–ø–∏—Å–æ–∫ –≤—Ä–µ–º–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π"):
        temporary_events_status(message)

    elif (message.text == "–ó–∞–≤–µ—Ä—à–∏—Ç—å —Å–æ–±—ã—Ç–∏–µ"):
        complete_event(message)

    elif (message.text == "–°–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ"):
        most_frequent_temporary_event(message)

    elif (message.text == "–°–∞–º–æ–µ —á–∞—Å—Ç–æ–µ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ"):
        most_frequent_constant_event(message)

    elif (message.text == "–°–ø–∏—Å–æ–∫ –ø–æ—Å—Ç–æ—è–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π"):
        constant_events_status(message)

    elif (message.text == "–£–¥–∞–ª–∏—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ"):
        event_del(message)

    elif (message.text == "–£–¥–∞–ª–∏—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ"):
        constant_event_del(message)

    elif (message.text == "–°–æ–∑–¥–∞—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ"):
        temporary_event_message(message)

    elif (message.text == "–°–æ–∑–¥–∞—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ"):
        constant_event_message(message)

    elif (message.text == "–ß—ë—Ç —Å–∫—É—à–Ω–æ —Å—Ç–∞–ª–æ, –±—Ä–æ—Å—å –∫—É–±–∏–∫  Ôºà‚óãÔø£ ÔΩû Ôø£‚óãÔºõÔºâH~m‚Ä¶"):
        dice(message)

    elif (message.text == "–ó–∞–¥–∞—Ç—å –≤–æ–ø—Ä–æ—Å"):
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        btn1 = types.KeyboardButton("–ö–∞–∫ —Ç–µ–±—è –∑–æ–≤—É—Ç?")
        btn2 = types.KeyboardButton("–ß—Ç–æ —Ç—ã –º–æ–∂–µ—à—å?")
        back = types.KeyboardButton("–í–µ—Ä–Ω—É—Ç—å—Å—è –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é")
        markup.add(btn1, btn2, back)
        bot.send_message(message.chat.id, text="–ó–∞–¥–∞–π –º–Ω–µ –≤–æ–ø—Ä–æ—Å", reply_markup=markup)

    elif (message.text == "–ö–∞–∫ —Ç–µ–±—è –∑–æ–≤—É—Ç?"):
        my_name = ['–£ –º–µ–Ω—è... –Ω–µ—Ç –∏–º–µ–Ω–∏ ‡ºº‚òØÔπè‚òØ‡ºΩ', '–Ø —Ç–æ—Ç, —á—å—ë –∏–º—è –Ω–µ–ª—å–∑—è –Ω–∞–∑—ã–≤–∞—Ç—å –≤ —Å–ª—É—Ö!!! (‚ó£‚óû‚ó¢ )',
                   '–∫—É–ª—å—Ç—É—Ä–Ω—ã–π —á–µ–ª–æ–≤–µ–∫ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –ø–µ—Ä–≤—ã–º (‚à© ‚å£ÃÄ‚Ä∏‚å£ÃÅ) ',
                   '–Ø - –ê–∏–Ω–∑ –û–∞–ª –ì–æ—É–Ω!!! (—É –º–µ–Ω—è –Ω–µ—Ç –∏–º–µ–Ω–∏, —ç—Ç–æ –ø—Ä–æ—Å—Ç–æ –æ—Ç—Å—ã–ª–∫–∞ –∫ –∞–Ω–∏–º–µ OverLord) (‚ÅÑ ‚ÅÑ‚Ä¢‚ÅÑœâ‚ÅÑ‚Ä¢‚ÅÑ ‚ÅÑ)',
                   '	(‚âñÔΩ§‚âñ‚ï¨) –≤–æ—Ç –∫–∞–∫ –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è —Ç–≤–æ–π –ø–∞–ª–µ—Ü –º–µ–∂–¥—É —Å—Ä–µ–¥–Ω–∏–º –∏ –º–∏–∑–∏–Ω—Ü–µ–º, —Ç–∞–∫ –º–µ–Ω—è –∏ –∑–æ–≤—É—Ç 	(‚âñÔΩ§‚âñ‚ï¨)',
                   '–≠—Ö... —Ç—ã –¥–∞–≤–∏—à—å –Ω–∞ –±–æ–ª—å–Ω–æ–µ ‚ï•Ôπè‚ï•',
                   '–Ø —Ç–µ–±–µ –Ω–µ —Å–∫–∞–∂—É, —Ç—ã –±–∞–∞–∞–∞–∫–∞ ( ‚âñ‡±™‚âñ )Âá∏ ']
        bot.send_message(message.chat.id, choice(my_name))

    elif message.text == "–ß—Ç–æ —Ç—ã –º–æ–∂–µ—à—å?":
        bot.send_message(message.chat.id, "1)–ü–æ–∑–¥–æ—Ä–æ–≤–∞—Ç—å—Å—è —Å —Ç–æ–±–æ–π")
        bot.send_message(message.chat.id, "2)–°–æ–∑–¥–∞—Ç—å —Å–æ–±—ã—Ç–∏–µ")
        bot.send_message(message.chat.id, '–ß—Ç–æ–±—ã —Å–æ–∑–¥–∞—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ, –≤–≤–µ–¥–∏ /temporary_event.')
        bot.send_message(message.chat.id, '–ß—Ç–æ–±—ã —Å–æ–∑–¥–∞—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ, –≤–≤–µ–¥–∏ /constant_event.')
        bot.send_message(message.chat.id,
                         '–ß—Ç–æ–±—ã –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å –Ω–∞ —Å–ø–∏—Å–æ–∫ –≤—Ä–µ–º–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π, –≤–≤–µ–¥–∏ /temporary_events_status')
        bot.send_message(message.chat.id,
                         '–ß—Ç–æ–±—ã –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å –Ω–∞ —Å–ø–∏—Å–æ–∫ –ø–æ—Å—Ç–æ—è–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π, –≤–≤–µ–¥–∏ /constant_events_status')
        bot.send_message(message.chat.id, '–ß—Ç–æ–±—ã —É–¥–∞–ª–∏—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ, –≤–≤–µ–¥–∏ /del_temporary_event.')
        bot.send_message(message.chat.id, '–ß—Ç–æ–±—ã —É–¥–∞–ª–∏—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ, –≤–≤–µ–¥–∏ /del_constant_event.')

    elif (message.text == "–í–µ—Ä–Ω—É—Ç—å—Å—è –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é"):
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        button1 = types.KeyboardButton("–ü—Ä–∏–≤–µ—Ç")
        button2 = types.KeyboardButton("–ó–∞–¥–∞—Ç—å –≤–æ–ø—Ä–æ—Å")
        button6 = types.KeyboardButton("–°–æ–∑–¥–∞—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
        button3 = types.KeyboardButton("–°–æ–∑–¥–∞—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
        button4 = types.KeyboardButton("–°–ø–∏—Å–æ–∫ –≤—Ä–µ–º–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π")
        button5 = types.KeyboardButton("–°–ø–∏—Å–æ–∫ –ø–æ—Å—Ç–æ—è–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π")
        button7 = types.KeyboardButton("–ó–∞–≤–µ—Ä—à–∏—Ç—å —Å–æ–±—ã—Ç–∏–µ")
        button8 = types.KeyboardButton("–£–¥–∞–ª–∏—Ç—å –≤—Ä–µ–º–µ–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
        button9 = types.KeyboardButton("–£–¥–∞–ª–∏—Ç—å –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–æ–±—ã—Ç–∏–µ")
        button10 = types.KeyboardButton("–ß—ë—Ç —Å–∫—É—à–Ω–æ —Å—Ç–∞–ª–æ, –±—Ä–æ—Å—å –∫—É–±–∏–∫  Ôºà‚óãÔø£ ÔΩû Ôø£‚óãÔºõÔºâH~m‚Ä¶")
        markup.add(button1, button2, button3, button4, button5, button6, button7, button8, button9, button10)
        bot.send_message(message.chat.id, text="–í—ã –≤–µ—Ä–Ω—É–ª–∏—Å—å –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é", reply_markup=markup)


if __name__ == '__main__':
    skuka = ['(¬¨‚Äø¬¨)', 'Œµ=Œµ=Œµ=Œµ=Œµ=Œµ= ‚óü( ‚ö´Õàœâ‚ö´Ã§)‚óû', 'Œ£Œ£‚â°‚â°‚â°ÔΩ∞(ÔΩ∞„ÉªœâÔΩ•)ÔΩ∞', '·ï¶ ï . ‚òØ ·¥• ‚òØ .  î·ï§', '(Ôºõ-‚óû‡±™‚óü-)', '( ‡πë‚ï•‚åì‚ï•)(‚óï‚åì‚óï ‚úø)',
             '‡∏Ö ï ·ìÄ ·¥• ·ìÇ  î‡∏Ö']
    bot.infinity_polling()
